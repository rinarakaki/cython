{
  "name": "cython",
  "displayName": "cython",
  "description": "Cython language support for Visual Studio Code",
  "private": true,
  "version": "0.0.1",
  "releaseTag": null,
  "publisher": "rnarkk",
  "repository": {
    "url": "https://github.com/rnarkk/cython.git",
    "type": "git"
  },
  "license": "MIT OR Apache-2.0",
  "keywords": [
    "cython"
  ],
  "categories": [
    "Formatters",
    "Programming Languages"
  ],
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": false,
      "description": "cython invokes binaries set up by its configuration as well as the Rust toolchain's binaries. A malicious actor could exploit this to run arbitrary code on your machine."
    }
  },
  "engines": {
    "vscode": "^1.94.0"
  },
  "enabledApiProposals": [],
  "scripts": {
    "package": "vsce package -o cython.vsix"
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.7.8",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "@vscode/vsce": "^3.2.0",
    "typescript": "^5.6.3"
  },
  "contributes": {
    "languages": [
      {
        "id": "cython",
        "extensions": [
          ".pyx",
          ".pxd",
          ".pxi"
        ],
        "aliases": [
          "Cython"
        ],
        "configuration": "language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "cython",
        "scopeName": "source.cython",
        "path": "cython.tmLanguage.json"
      }
    ],
    "semanticTokenScopes": [
      {
        "language": "cython",
        "scopes": {
          "keyword": [
            "keyword.other.cython"
          ]
        }
      }
    ]
  }
}
